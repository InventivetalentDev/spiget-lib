{
  "name": "spiget",
  "version": "0.0.3",
  "description": "Javascript/Typescript client for the spiget.org API",
  "main": "index.js",
  "scripts": {
    "test": "mocha test/**/*.ts --require ts-node/register",
    "compile": "tsc",
    "generate-types": "node ./dist/generator/src/generator",
