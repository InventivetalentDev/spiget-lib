    "axios": "0.19.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "14.0.14",
    "chai": "4.2.0",
    "mocha": "8.0.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  }
}
