{
  "name": "spiget",
  "version": "0.0.3",
  "description": "Javascript/Typescript client for the spiget.org API",
  "main": "index.js",
  "files": [
    "dist/**/*",
    "example.js"
  ],
  "scripts": {
    "test": "mocha test/**/*.ts --require ts-node/register",
    "compile": "tsc",
    "generate-types": "ts-node src/generator",
    "apply": "ts-node src/generator/apply",
    "clear-generated-types": "rm -rf src/generated_types",
    "build": "rm -rf dist && npm run clear-generated-types && npm run generate-types && npm run apply && npm run compile"
  },
  "author": "Haylee Sch√§fer",
  "license": "MIT",
  "dependencies": {
    "axios": "0.19.2"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/chai": "4.2.11",
    "@types/mocha": "7.0.2",
    "@types/node": "14.0.14",
    "chai": "4.2.0",
    "mocha": "8.0.1",
    "ts-node": "8.10.2",
    "typescript": "3.9.6"
  }
}
